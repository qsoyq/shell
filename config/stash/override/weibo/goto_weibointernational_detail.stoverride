name: 微博国际版URLScheme
desc: |-
  - 捕获微博博文请求并发出对应的跳转通知
category: qsoyq
icon: https://cdn.jsdelivr.net/gh/Koolson/Qure@master/IconSet/Color/Weibo.png
http:
  mitm:
    - weibo.com
    - m.weibo.cn
  url-rewrite:
  script:
    - match: https://(weibo.com|m.weibo.cn)
      name: "微博国际版URLScheme"
      type: request
      require-body: false
      timeout: 5
      argument: ""

script-providers:
  "微博国际版URLScheme":
    url: https://qsoyq-public.oss-cn-shanghai.aliyuncs.com/github/shell/config/stash/script/weibo/goto_weibointernational_detail.js
    interval: 86400
